{
  "name": "jeniferai",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/core/*",
    "packages/modules/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "db:migrate": "turbo db:migrate",
    "db:types": "turbo db:types"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@ai-sdk/react": "^3.0.41",
    "@react-stately/utils": "^3.10.8",
    "@untitledui/icons": "^0.0.19",
    "ai": "^4.0.0",
    "react-aria-components": "^1.13.0",
    "tailwind-merge": "^3.3.1"
  }
}
